# FiftyOne Manager Configuration

# Base path for all datasets
DATASET_BASE_PATH=/data/datasets

# Port range for FiftyOne instances
PORT_START=5151
PORT_END=5160

# Manager API port
MANAGER_PORT=5000

# Public address for accessing the manager and label editor
# Use the IP/hostname that users access FiftyOne from
# Examples: localhost, 192.168.1.100, myserver.local
PUBLIC_ADDRESS=localhost

# Default IoU threshold for duplicate detection (0.0 - 1.0)
DEFAULT_IOU_THRESHOLD=0.25

# Default debug mode (true/false)
DEFAULT_DEBUG_MODE=false

# Health check interval in milliseconds (how often to check if FiftyOne service is responding)
HEALTH_CHECK_INTERVAL=5000

# Health check timeout in milliseconds (how long to wait for FiftyOne service response)
HEALTH_CHECK_TIMEOUT=3000

# CVAT Integration Configuration
# CVAT server URL (can be app.cvat.ai or self-hosted server)
CVAT_URL=http://192.168.20.3:5100/
# CVAT username for authentication
CVAT_USERNAME=alei
# CVAT password for authentication
CVAT_PASSWORD=studio0000
# CVAT email (if applicable)
CVAT_EMAIL=

# Label Studio Integration Configuration
# Label Studio server URL (self-hosted or cloud)
LABELSTUDIO_URL=http://localhost:8080/
# Label Studio API key for authentication
# Get this from: Settings > Account & Settings > Access Token
LABELSTUDIO_API_KEY=
# Batch size for uploading samples (default: 100)
# Smaller batches = more reliable, larger batches = faster
LABELSTUDIO_BATCH_SIZE=100
# Timeout for Label Studio API operations in seconds (default: 86400 = 24 hours)
# Increase this if uploading large batches or slow network
LABELSTUDIO_TIMEOUT=86400
# Optional: Local file storage configuration
# Enable local file serving in Label Studio
LABELSTUDIO_LOCAL_FILES_SERVING_ENABLED=false
# Path to serve local files from (if enabled)
LABELSTUDIO_LOCAL_FILES_DOCUMENT_ROOT=/data/datasets
